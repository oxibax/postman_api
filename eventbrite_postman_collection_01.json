		{
			"name": "GET: All Events By Organization ID",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", () => {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"const response = pm.response.json();",
							"const events = response.events;",
							"const event = events[0];",
							"",
							"if (event) {",
							"    pm.globals.set(\"event_id\", event.id);",
							"}",
							"",
							"pm.test(\"Organization has events\", () => {",
							"    pm.expect(events).length.to.be.above(0);",
							"});",
							"",
							"pm.test(\"Event found\", () => {",
							"    pm.expect(event).to.be.an('object');",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
